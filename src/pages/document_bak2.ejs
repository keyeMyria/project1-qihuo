<script src="./polyfill.min.js"></script>
<script>
    $('.k').css('position','fixed');
    $('.k').css('top','82px');
    $('.k').css('left','0');
    // $('.k').css('display','none');
    $('.k').width(window.screen.width + "px");
    $('.k').height("0px");
    var r_code = '',k_frame,test;
    function initK(type,code){
        console.log(123)
        if(r_code != code){
            r_code = code;
            k_frame = $(`<iframe src="http://web.hdlkg.net/k2.html?type=${type}&code=${code}" frameborder="0" class="k"></iframe>`);
            k_frame.width(window.screen.width + "px");
            k_frame.height(window.screen.height -327 + "px")
            $('#k-frame').append(k_frame.clone());
            $('#test').append(k_frame.clone());
        }else{
            console.log($('#test').html());
            $('#k-frame').html($('#test').html());
            $('.k').height( "100px")
        }
    }
    function showK(index){
        $('.k').eq(index)
            // .height(document.documentElement.clientHeight -87-82-143 -16 + "px")
            .height(window.screen.height -327 + "px")
            .siblings('.k')
            .height('0px');
    }
    function hideK() {
        $('.k').height('0px');
    }
</script>
